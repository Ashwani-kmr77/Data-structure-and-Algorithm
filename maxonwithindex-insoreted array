#include<iostream>
#include<bits/stdc++.h>
using namespace std;
 int main(){
    int n;
    cin>>n;
    int arr[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cin>>arr[i][j];
        }
    }

 cout<<endl;
 int first;
 int maxone=-1;
 int count=0;
 int rowindex = -1;
 
  for(int i=0;i<n;i++){
    int low=0;
    int high=n-1;
    while(low<=high){

        int mid = low+(high-low)/2;
        if(arr[i][mid]==1){
            first=mid;
            high=mid-1;
        }
        else low=mid+1;
    }
    count = n-first;
    if(count>maxone){
        rowindex=i;
    }
    maxone = max(count,maxone);
  
  }
cout<<"rowindex is :"<<rowindex;
cout<<endl;
cout<<"maxone is :"<<maxone;
 }
