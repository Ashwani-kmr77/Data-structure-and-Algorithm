#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
using namespace std;
int main(){
    string s;
    getline(cin,s);
    string str = "";
    int n=s.size();
    for(int i=0;i<n;i++){
        if(s[i]>='X'){
            str+=s[i];
        }
    }
    //sort(str.begin(),str.end());
      for(int i=0;i<str.size()-1;i++){

        for(int j=0;j<str.size()-1-i;j++){
            if(str[j]>str[j+1]){
                swap(str[j],str[j+1]);
            }
        }
      }

    for(int i=str.size();i>=0;i--){
        cout<<str[i];
    }



}
