class Solution {
public:
    int arrangeCoins(int n) {
        long long low=0;
        long long high = n;

        while(low<=high){
        long long mid = low+(high-low)/2;
        long long coinused = mid*(mid+1)/2;

        if(coinused == n){
          return  mid;
        }
        else if(coinused>n){
         high=mid-1;
        }
        else {
       low = mid+1;
        }
        }
        return high;
    }
};
