class Solution {
public:
        bool ispalindrome(string&s ,int l, int r){
            while(l<r){
            if(s[l]==s[r]){
                l++;
                r--;
            }
            else return false;
            }
            return true;
        }
    
    vector<vector<string>>result;
     void solve(string s ,vector<string>&temp, int idx ){
      if(idx>=s.size()){
        result.push_back(temp);
        return;
      } 


     for(int i =idx;i<s.size();i++){
        if(ispalindrome(s,idx,i)){
        // do 
          temp.push_back(s.substr(idx,i-idx+1)); // substring store here
      // explore
          solve(s,temp,i+1);
      // undo 
          temp.pop_back();
   
         }
       }
     }
    vector<vector<string>> partition(string s) {
        vector<string>temp;
        solve(s,temp,0);
        return result;
        
    }
};
