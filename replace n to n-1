#include<iostream>
#include<vector>
#include<climits>
using namespace std;
 int main(){
    int n;
    cin>>n;
    int arr[n];
    int x=0;
    vector<int>v(n,0);
    for(int i=0;i<n;i++){
       cin>>arr[i];
        }

    for(int i=0;i<n;i++){
        int min= INT_MAX;
        int minindex ;
        for(int j=0;j<n;j++){
            if(v[j]==1) continue;
            else 
            {
                if(min>arr[j]){
                    min = arr[j];
                    minindex =j;
                }
            }
        }
         arr[minindex]=x;
         v[minindex]=1;
         x++;
    }
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
 }
