#include<iostream>
using namespace std;
int main(){
    int arr[6]={0,0,0,1,1,1};
     int count=0;
     int n=6;
     int low=0;
     int high= n-1;
     int first=-1;
     while(low<=high){
       int mid = low+(high-low);
       if(arr[mid]==1){
        first=mid;
        high=mid-1;
       }
     else{
            low=mid+1;
     }
    }
    cout<<n-first;

    }
       
