class Solution {
public:
    TreeNode*deletehelpher(TreeNode* root , unordered_set<int>&set, vector<TreeNode*>&result){

        if(root==NULL) return NULL;
        root->left  = deletehelpher(root->left,set,result);
        root->right = deletehelpher(root->right,set,result);
        if(set.find(root->val) != set.end()){
            if(root->left !=NULL){
                result.push_back(root->left);
              }
            if(root->right != NULL){
                result.push_back(root->right);
              }
          return NULL;
        }

         else{
                return root; 
             }
         }

    vector<TreeNode*> delNodes(TreeNode* root, vector<int>& to_delete) {
        vector<TreeNode*>result;
        unordered_set<int>set;
        for(int &num : to_delete){
            set.insert(num);
        }
        deletehelpher(root,set,result); // result will not consider root

      if(set.find(root->val)==set.end())   {
        result.push_back(root);
      }         

      return result;                    
    }
};
